implementation

function IsKnownEntityName(const UnitTypeName: string): Boolean;
var
  I: Integer;
begin
  Result := False;

  // Empty string is not an entity
  if UnitTypeName = '' then
    Exit;

  // Linear search through known entity names
  for I := Low(KNOWN_ENTITY_NAMES) to High(KNOWN_ENTITY_NAMES) do
  begin
    if SameText(UnitTypeName, KNOWN_ENTITY_NAMES[I]) then
    begin
      Result := True;
      Exit;
    end;
  end;
end;

end.
